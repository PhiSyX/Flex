{
	"name": "@phisyx/flex-chat-webapp",
	"private": true,
	"version": "0.1.0",
	"type": "module",
	"scripts": {
		"dev": "vite --config config/vite.config.ts",
		"story:dev": "histoire dev -c config/histoire.config.ts",
		"build": "vite build --config config/vite.config.ts",
		"lint": "vue-tsc -p config/tsconfig.app.json",
		"preview": "vite preview -c config/vite.config.ts",
		"test:e2e": "playwright test -x -c config/playwright.config.ts",
		"test:e2e:report": "playwright show-report tests/e2e/report"
	},
	"dependencies": {
		"@phisyx/flex-capitalization": "workspace:*",
		"@phisyx/flex-css": "workspace:*",
		"@phisyx/flex-date": "workspace:*",
		"@phisyx/flex-safety": "workspace:*",
		"@phisyx/flex-search": "workspace:*",
		"@phisyx/flex-uikit": "workspace:*",
		"focus-trap": "^7.5.4",
		"pinia": "^2.1.7",
		"socket.io-client": "^4.7.5",
		"vue": "^3.4.27"
	},
	"devDependencies": {
		"@histoire/plugin-vue": "^0.17.17",
		"@playwright/test": "^1.44.1",
		"@tsconfig/node20": "^20.1.4",
		"@types/node": "^20.14.0",
		"@vitejs/plugin-vue": "^5.0.5",
		"histoire": "^0.17.17",
		"sass": "^1.77.4",
		"typescript": "^5.4.5",
		"vite": "^5.2.12",
		"vue-tsc": "^2.0.19",
		"yaml": "^2.4.3"
	}
